<?xml version="1.0" encoding="UTF-8"?>
<component name="netifera.spider-daemon-factory">
	<implementation class="com.netifera.platform.net.http.internal.spider.WebSpiderDaemonFactory" />
	<service>
		<provide interface="com.netifera.platform.net.http.spider.IWebSpiderDaemonFactory" />
	</service>
	
   <reference
    	name="DISPATCHER"
    	interface="com.netifera.platform.api.dispatcher.IMessageDispatcherService"
    	bind="setDispatcher"
    	unbind="unsetDispatcher"
    />
    
	<reference
    	name="LOG_MANAGER"
    	interface="com.netifera.platform.api.log.ILogManager"
    	bind="setLogManager"
    	unbind="unsetLogManager"
    />
</component>