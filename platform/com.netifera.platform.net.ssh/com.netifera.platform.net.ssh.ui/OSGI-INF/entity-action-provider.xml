<?xml version="1.0" encoding="UTF-8"?>
<component name="netifera.ssh-action-provider">
	<implementation class="com.netifera.platform.net.ssh.internal.ui.EntityActionProvider" />
	<service>
		<provide interface="com.netifera.platform.ui.api.actions.IEntityActionProvider" />
	</service>
	
	<reference 
		name="WORDLISTS"
		interface="com.netifera.platform.net.wordlists.IWordList"
		bind="registerWordList"
		unbind="unregisterWordList"
		cardinality="0..n"
		policy="dynamic"
	/>
	
	<reference 
		name="PROBE_BUILDER"
		interface="com.netifera.probebuild.api.IProbeBuilderService"
		bind="setProbeBuilder"
		unbind="unsetProbeBuilder"
	/>
</component>
